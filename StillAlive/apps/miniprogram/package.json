    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "4.0.0",
    "@tarojs/webpack5-runner": "4.0.0",
    "@types/react": "^18.3.0",
    "babel-preset-taro": "4.0.0",
    "eslint": "^8.57.0",
    "postcss": "^8.4.35",
    "sass": "^1.80.0",
    "typescript": "^5.3.0"
