    "@tarojs/runtime": "4.0.0",
    "@tarojs/shared": "4.0.0",
    "@tarojs/taro": "4.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "zustand": "^5.0.0",
    "dayjs": "^1.11.0"
